{
    "manifest_version": 3,
    "name": "QuickToS",
    "version": "1.0",
    "description": "Generates summaries of Terms of Service agreements.",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "icon.png"
    }
  }
  